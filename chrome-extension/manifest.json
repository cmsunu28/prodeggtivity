{
  "manifest_version": 2,

  "name": "prodEggtivity",
  "description": "a chrome extension to set off a clicker on the game Egg, Inc when you are browsing work-related tabs",
  "version": "0.0.1",
  "options_page": "options.html",
  "background": {
    "scripts" : ["background.js"],
    "persistent": false
    },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "webNavigation",
    "https://api.particle.io/v1/devices/*",
    "http://*/",
    "https://*/*"
  ]
}